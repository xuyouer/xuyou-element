{
  "name": "xuyou-element",
  "version": "1.0.10",
  "description": "Components library by Vite + Vue3 + TS",
  "type": "module",
  "main": "./dist/umd/index.umd.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/types/core/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/umd/index.umd.cjs",
      "types": "./dist/types/core/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "sideEffects": [
    "./dist/index.css"
  ],
  "scripts": {
    "build": "run-s clean build-only move-style",
    "build-only": "run-p build-es build-umd",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "move-style": "move-file dist/es/index.css dist/index.css",
    "clean": "rimraf dist",
    "release": "release-it"
  },
  "keywords": [
    "kaloo",
    "xiaomizha",
    "xuyou",
    "js",
    "xuyou-element",
    "element",
    "组件库",
    "前端组件库"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5"
  },
  "devDependencies": {
    "vite-plugin-dts": "^4.4.0",
    "@xuyou-element/components": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xuyouer/xuyou-element/"
  },
  "bugs": {
    "url": "https://github.com/xuyouer/xuyou-element/issues"
  },
  "homepage": "https://xuyouer.github.io/xuyou-element/"
}
